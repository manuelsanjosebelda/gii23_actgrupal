        "## Función para calcular e\n",
        "\n",
        "\n",
        "\n"
      ],
      "metadata": {
        "id": "kYtVwXmxF6Ql"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def calcular_e(aproximaciones):\n",
        "    e_aproximado = 0.0\n",
        "    for i in range(aproximaciones):\n",
        "        e_aproximado += 1 / factorial(i)\n",
        "    return e_aproximado\n",
        "def factorial(n):\n",
        "    if n == 0:\n",
        "        return 1\n",
        "    else:\n",
        "        return n * factorial(n-1)"
      ],
